{
    "name": "graham-campbell/htmlmin",
    "description": "HTMLMin Is A Simple HTML Minifier For Laravel 4.2",
    "keywords": ["laravel", "framework", "html minifier", "html", "minifier", "HTMLMin", "Laravel HTMLMin", "Laravel-HTMLMin", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/filesystem": "~4.2",
        "illuminate/http": "~4.2",
        "illuminate/routing": "~4.2",
        "illuminate/support": "~4.2",
        "illuminate/view": "~4.2",
        "mrclay/minify": "~2.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\HTMLMin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\HTMLMin\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
